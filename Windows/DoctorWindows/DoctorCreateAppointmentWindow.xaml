<Window x:Class="SF_16_POP2020.Windows.DoctorWindows.DoctorCreateAppointmentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SF_16_POP2020.Windows.DoctorWindows"
        mc:Ignorable="d"
        Title="Doctor Create Appointment Window" Height="450" Width="300">
    <Grid>
        <DatePicker x:Name="DatePicker" HorizontalAlignment="Stretch" Margin="0,20,0,0" VerticalAlignment="Top"/>
        <Button Click="Create_OnClick" Content="Create" HorizontalAlignment="Right" Margin="0,389,10,0" VerticalAlignment="Top" Width="75"/>
        <Button Click="Back_OnClick"  Content="Back" HorizontalAlignment="Left" Margin="10,389,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
