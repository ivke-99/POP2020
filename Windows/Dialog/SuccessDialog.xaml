<Window x:Class="SF_16_POP2020.Windows.Dialog.SuccessDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SF_16_POP2020.Windows.Dialog"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
          Title="Success" Height="200" Width="400">
    <Grid>
        <Button Content="OK" HorizontalAlignment="Stretch" Margin="0,123,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <TextBlock x:Name="tbSuccess" HorizontalAlignment="Stretch" Margin="20,65,20,0" TextWrapping="Wrap" Text="Success" VerticalAlignment="Top"
                   TextAlignment="Center" FontSize="18px"/>

    </Grid>
</Window>
